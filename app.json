{
  "expo": {
    "name": "Le GANDA",
    "slug": "leganda",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#30698C"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Autorisez LE GANDA à accéder à votre caméra",
          "photosPermission": "Autorisez LE GANDA à accéder à vos photos"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cs.leganda",
      "config": {
        "googleMapsApiKey": "AIzaSyCaLoZk5uf3Prv3xIiuQgI2GO8AyaWoZGE"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCaLoZk5uf3Prv3xIiuQgI2GO8AyaWoZGE"
        }
      },
      "package": "com.cs.leganda",
      "versionCode": 35,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#30698C"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
