{
  "expo": {
    "name": "mobile-leganda",
    "slug": "mobile-leganda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#30698C"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Autorisez LE GANDA à accéder à votre caméra",
          "photosPermission": "Autorisez LE GANDA à accéder à vos photos"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
